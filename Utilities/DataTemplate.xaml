<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:Modern_Real_Estate.ViewModel"
                    xmlns:view="clr-namespace:Modern_Real_Estate.View"
                    >


    <DataTemplate DataType="{x:Type viewModel:HomeViewModel}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:ApartmentViewModel}">
        <view:ApartmentView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:ShopViewModel}">
        <view:ShopView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:HospitalViewModel}">
        <view:HospitalView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:SchoolViewModel}">
        <view:SchoolView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:TownhouseViewModel}">
        <view:TownhouseView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:UniversityViewModel}">
        <view:UniversityView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:VillaViewModel}">
        <view:VillaView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:WarehouseViewModel}">
        <view:WarehouseView />
    </DataTemplate>
</ResourceDictionary>